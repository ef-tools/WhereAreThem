<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="path" value="D:\Sync\Software\WhereAreThem\lists"/>
    <add key="enableWatcher" value="false"/>
    <add key="enableCompression" value="true"/>
    <!-- alternative values for persistenceType: bin, txt, jsonNet, json or xml -->
    <add key="persistenceType" value="bin"/>
    <!-- driveTypes can be a combination of: Fixed,Removable,Network,CDRom,Ram -->
    <add key="driveTypes" value="Fixed,Network"/>
    <!-- scannerFilters can be a combination of: Hidden,ReparsePoint,System -->
    <add key="scannerFilters" value="Hidden,ReparsePoint"/>
    <add key="navigationHistoryCount" value="20"/>
    <add key="MediaInfoPlugin" value=".mkv|.mp4|.avi|.wmv|.flv|.webm"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
